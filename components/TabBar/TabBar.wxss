/* components/TabBar/TabBar.wxss */

.tab_title {
  display: flex;
  justify-content: center;
  align-items: center;
}
.tab_header {
  flex: 1;
  text-align: center;

  padding: 20rpx 0;
  border-bottom: 4rpx solid #ddd;
}

.active {
  color: var(--themColor);
  border-bottom: 4rpx solid var(--themColor);
}
