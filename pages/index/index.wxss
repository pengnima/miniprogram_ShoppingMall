/* 轮播图样式 */
.home_swiper swiper {
  height: 340rpx;
}
.home_swiper swiper image {
  width: 100%;
}

/* 导航样式 */
.home_cates {
  display: flex;
  /* width: 100%; */
  /* justify-content: space-around; */
}
.home_cates navigator {
  flex: 1;
  padding: 30rpx;
}
.home_cates image {
  width: 100%;
}

/* 楼层样式 */
.floor_group {
  overflow: hidden;

  padding: 10rpx;
}
.floor_title {
  width: 100%;
}
.floor_title image {
  width: 100%;
}

.floor_list navigator {
  float: left;
  width: 33.33%;
}
.floor_list navigator:nth-child(n + 2) {
  height: 207.97rpx;

  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3) {
  border-bottom: 5rpx solid #fff;
}
.floor_list navigator:nth-child(4),
.floor_list navigator:nth-child(5) {
  border-top: 5rpx solid #fff;
}
.floor_list navigator image {
  width: 100%;
  height: 100%;
}
